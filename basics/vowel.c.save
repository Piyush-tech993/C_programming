#include<stdio.h>
int main()
{
	int vowels =0 , constants =0;
	int i;
	int ch;
	int str;
	for(i=0; str[i]=0;i++)
	{
		ch = str[i];
		if(ch=='a' || ch=='A' || ch=='E'|| ch=='e'||ch=='I'||ch=='i'||
	ch=='O'||ch=='o'||ch=='U'||ch=='u')
	vowels ++;
	else if (ch==' ')
	continue;
	else
	 constants++;
}
	 printf("%d are the no.of vowels\n",vowels);
  	 printf("%d are the no. of constants\n",constants);
}

